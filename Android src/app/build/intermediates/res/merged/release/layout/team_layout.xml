<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="אבי כהן"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="152dp"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/img_meitar"
        android:layout_alignStart="@+id/img_avi"
        android:layout_alignEnd="@+id/img_avi" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="שגיא מרסיאנו"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="28dp"
        android:layout_marginTop="76dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/img_sagi"
        android:layout_alignEnd="@+id/img_sagi" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="מיתר שוקרון"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="28dp"
        android:layout_above="@+id/img_meitar"
        android:layout_alignStart="@+id/img_meitar"
        android:layout_marginBottom="16dp"
        android:layout_alignEnd="@+id/img_meitar" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="85dp"
        android:layout_height="14dp"
        android:text="זאב מלומיאן"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="275dp"
        android:layout_alignBottom="@+id/textView14"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/img_avi" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="עמרי טרם"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="310dp"
        tools:layout_editor_absoluteY="275dp"
        android:layout_alignBaseline="@+id/textView14"
        android:layout_alignBottom="@+id/textView14"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/img_sagi"
        android:layout_alignEnd="@+id/img_terem" />

    <ImageView
        android:id="@+id/img_meitar"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@color/caldroid_white"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="60dp"
        android:layout_alignTop="@+id/img_sagi"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="59dp" />

    <ImageView
        android:id="@+id/img_terem"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@color/caldroid_white"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="306dp"
        android:layout_alignTop="@+id/img_avi"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/img_zeev"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@color/caldroid_white"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="306dp"
        android:layout_marginTop="46dp"
        android:layout_below="@+id/img_meitar"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/img_sagi"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@color/caldroid_white"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="60dp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/textView18"
        android:layout_toStartOf="@+id/textView19" />

    <ImageView
        android:id="@+id/img_avi"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@color/caldroid_white"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="189dp"
        android:layout_alignTop="@+id/img_zeev"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="צוות הפיתוח"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp" />
</RelativeLayout>